#include <iostream>

using namespace std;

int main()
{
	
	float number1, number2, result;
	
	cout << "\n Enter first number : ";
	cin >> number2;
	
	cout << "\n Enter first number : ";
	cin >> number2;
	
	result = number1 / number2;
	
	cout << "\n The result iod division " << number1 << "divided by " << number2 << " is: " << result;
	
	return 0;
}
